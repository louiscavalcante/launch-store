	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"connect-pg-simple": "^7.0.0",
		"express": "^4.17.1",
		"express-session": "^1.17.2",
		"method-override": "^3.0.0",
		"multer": "^1.4.3",
		"nunjucks": "^3.2.3",
		"pg": "^8.7.1"
	},
	"devDependencies": {
		"browser-sync": "^2.27.5",
		"cross-env": "^7.0.3",
		"nodemon": "^2.0.13",
		"npm-run-all": "^4.1.5"
	}
}

{
	"name": "launch-store",
	"version": "1.0.0",
	"description": "",
	"main": "server.js",
	"scripts": {
		"debug": "cross-env APP_ENV=development nodemon --inspect=0.0.0.0:9229 src/server.js",
		"start": "node src/server.js",
		"dev": "npm-run-all -p nodemon browser-sync",
		"nodemon": "cross-env APP_ENV=development nodemon --watch public --watch src --watch package.json src/server.js",
